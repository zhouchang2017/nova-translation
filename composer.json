{
    "name": "chang/nova-translation",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "dimsav/laravel-translatable": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Chang\\NovaTranslation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chang\\NovaTranslation\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
